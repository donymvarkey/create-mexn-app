{
  "name": "create-mexn-app",
  "version": "1.0.9",
  "main": "dist/cli.js",
  "type": "module",
  "description": "A CLI tool to scaffold a new MongoDB-Express-Node app. Inspired from create-react-app",
  "bin": {
    "create-mexn-app": "dist/cli.js"
  },
  "scripts": {
    "start": "node dist/cli.js test-project",
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "nodejs-express-mongodb",
    "mongodb",
    "express",
    "template",
    "scaffold",
    "javascript",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/donymvarkey/create-mexn-app.git"
  },
  "bugs": {
    "url": "https://github.com/donymvarkey/create-mexn-app.git"
  },
  "homepage": "https://github.com/donymvarkey/create-mexn-app.git#README",
  "author": "Dony M Varkey",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/degit": "^2.8.6",
    "@types/figlet": "^1.7.0",
    "@types/node": "^22.10.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "prettier": "3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "degit": "^2.8.4",
    "figlet": "^1.8.0",
    "inquirer": "^12.3.3",
    "ora": "^8.1.1",
    "simple-git": "^3.27.0"
  }
}
