{
  "name": "create-mexn-app",
  "version": "1.0.14",
  "main": "dist/cli.js",
  "type": "module",
  "description": "A CLI tool to scaffold a new MongoDB-Express-Node app. Inspired from create-react-app",
  "bin": {
    "create-mexn-app": "dist/cli.js"
  },
  "scripts": {
    "start": "node dist/cli.js test-project",
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "nodejs-express-mongodb",
    "mongodb",
    "express",
    "template",
    "scaffold",
    "javascript",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/donymvarkey/create-mexn-app.git"
  },
  "bugs": {
    "url": "https://github.com/donymvarkey/create-mexn-app.git"
  },
  "homepage": "https://github.com/donymvarkey/create-mexn-app.git#README",
  "author": "Dony M Varkey",
  "license": "MIT",
  "engines": {
    "node": ">=22.16.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/degit": "^2.8.6",
    "@types/figlet": "^1.7.0",
    "@types/node": "^24.0.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "prettier": "3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "degit": "^2.8.4",
    "figlet": "^1.8.1",
    "inquirer": "^12.6.3",
    "ora": "^8.2.0"
  }
}
